{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2021"],
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "types": ["react", "react-native"],
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["App.tsx", "src", "index.js"],
  "exclude": ["node_modules"]
}






